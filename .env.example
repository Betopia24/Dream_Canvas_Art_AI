# API Keys (Required)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_SECRET=your_gemini_api_secret_here
OPEN_AI_API_KEY=your_openai_api_key_here
HUGGINGFACE_TOKEN=your_huggingface_token_here
FAL_API_KEY=your_fal_api_key_here

# Stable Diffusion Model
STABLE_DIFFUSION_MODEL_ID=stabilityai/stable-diffusion-3.5-large
STABLE_DIFFUSION_DEVICE=cuda

# Image Generation
IMAGE_WIDTH=1024
IMAGE_HEIGHT=1024
NUM_INFERENCE_STEPS=50
GUIDANCE_SCALE=7.0
NEGATIVE_PROMPT=blurry, low quality, distorted, deformed


# Output
IMAGES_DIR=generated_images
BASE_URL=http://localhost:8000

# Cloud Storage Configuration
CLOUD_STORAGE_PROVIDER=local  # Options: local, aws, gcs, azure

# Google Cloud Storage Configuration (if using GCS)
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json
GCS_BUCKET_NAME=your-gcs-bucket-name

